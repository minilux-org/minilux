# Test File I/O
$w_res = write_file("test_io.txt", "Hello IO")
printf("Write result: ", $w_res, "\n")

$content = read_file("test_io.txt")
printf("Read content length: ", len($content), "\n")

if ($content == "Hello IO") {
    printf("File I/O: OK\n")
} else {
    printf("File I/O: FAILED (got '", $content, "')\n")
}

# Cleanup
write_file("test_io.txt", "")
